{% extends 'admin_base.html.twig' %}

{% block title %}eventName index{% endblock %}

{% set page = "eventName" %}

{% block body %}
    <h1>eventName index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for eventName in categories %}
            <tr>
                <td>{{ eventName.id }}</td>
                <td>{{ eventName.name }}</td>
                <td>
                    <a href="{{ path('eventName_show', {'id': eventName.id}) }}">show</a>
                    <a href="{{ path('eventName_edit', {'id': eventName.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('eventName_new') }}">Create new</a>
{% endblock %}
